
<p>
  There are <strong><u><%= @contacts.count%></u></strong> contacts
</p>
<% @contacts.each do |contact| %>
<div class="all_contacts">
  <ul>
    <li><a href="/contacts/<%=contact.id%>"><%=contact.full_name %></a> (<%= contact.email%>)</li>
    <a href="/contacts/<%= contact.id%>/edit"> Edit </a>
    |
    <a href="/contacts/<%= contact.id%>/edit"> Delete </a>
  </ul>
</div>
<%end%>
